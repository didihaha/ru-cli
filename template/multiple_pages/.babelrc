{
    "presets": ["env"],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        [
        "transform-class-properties",
        {
            "spec": true
        }
        ]
    ]
}
      